<div id="carouselExampleControls" class="carousel slide" data-bs-ride="carousel">
    <ol class="carousel-indicators">
        <li data-bs-target="#carouselExampleControls" data-bs-slide-to="0" class="active"></li>
        <li data-bs-target="#carouselExampleControls" data-bs-slide-to="1"></li>
        <li data-bs-target="#carouselExampleControls" data-bs-slide-to="2"></li>
    </ol>
    <div class="carousel-inner">
        <h1 style="    z-index: 999;
        position: absolute;
        top: 136px;
        left: 33px; color: white;
        -webkit-text-fill-color: white; /* Will override color (regardless of order) */
        -webkit-text-stroke-width: 1px;
        -webkit-text-stroke-color: red;
        font-size: 54px;">Ensemble Choisissons !</h1>
        <div class="carousel-item active">
            <img src="/images/piscine.jpg" class="d-block w-100 " alt="piscine">
            
            <div class="box-text-img">
                <h2 class="img-titre-index">Piscine</h2>
                <p class="img-text-index">Viens faire un plongeons à la piscine municipale de Fernelmont avec tout tes amis!</p>
            </div>
        </div>
        <div class="carousel-item">
            <img src="/images/peinture.jpg" class="d-block w-100 " alt="peinture">
            <div class="box-text-img">
                <h2 class="img-titre-index">Artiste</h2>
                <p class="img-text-index">Exposition d'un jeune artiste Fernelmontois, Pierrick Purnode</p>
            </div>
        </div>
        <div class="carousel-item">
            <img src="/images/cinema.jpg" class="d-block w-100 " alt="cinema">
            <div class="box-text-img">
                <h2 class="img-titre-index">Outdoor cinéma</h2>
                <p class="img-text-index">Projection du tout dernier film X-MEN</p>
            </div>
        </div>
    </div>
</div>

